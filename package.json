{
  "name": "personal-notes-app",
  "version": "1.0.0",
  "license": "0BSD",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "build:web": "npx expo export:web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "npm run build:web && npm run build:android && npm run build:ios",
    "preview": "npx expo start --tunnel",
    "publish": "npx expo publish",
    "clean": "rm -rf node_modules && npm install",
    "type-check": "tsc --noEmit",
    "lint": "npx eslint . --ext .ts,.tsx",
    "format": "npx prettier --write .",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@expo/vector-icons": "latest",
    "@react-native-async-storage/async-storage": "latest",
    "@react-native-community/slider": "4.5.7",
    "@react-navigation/bottom-tabs": "7.4.2",
    "@react-navigation/native": "7.1.14",
    "@react-navigation/native-stack": "7.3.21",
    "@supabase/supabase-js": "2.50.2",
    "expo": "^52.0.46",
    "expo-camera": "16.1.9",
    "expo-linear-gradient": "14.1.5",
    "lucide-react-native": "0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "4.11.1",
    "react-native-svg": "15.12.0",
    "react-native-web": "~0.19.6",
    "sonner-native": "0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^19.1.0",
    "typescript": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react-native",
    "expo",
    "notes",
    "personal",
    "arabic",
    "mobile-app"
  ],
  "author": "ali",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/personal-notes-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/personal-notes-app/issues"
  },
  "homepage": "https://github.com/yourusername/personal-notes-app#readme"
}
